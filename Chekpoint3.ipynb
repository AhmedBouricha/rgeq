    "def multiply_list(items):\n",
    "    a = 1\n",
    "    for x in items:\n",
    "        a *= x\n",
    "    return a\n",
    "\n",
    "b = []\n",
    "n = int(input(\"Entrez le nombre d'éléments de la liste: \"))\n",
    "\n",
    "for i in range(0, n):\n",
    "    c = int(input())\n",
    "    b.append(c)\n",
    "\n",
    "print(\"La liste est: \", b)\n",
    "print(\"Le résultat est:\", multiply_list(b))\n"
   ]
